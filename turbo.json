{
	"$schema": "https://turbo.build/schema.json",
	"globalDependencies": ["tsconfig.base.json", "tsconfig.base.server.json", "tsconfig.base.client.json"],
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**", ".next/**"]
		},
		"build:commerce": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**", ".next/**"]
		},
		"build:definition": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**", ".next/**"]
		},
		"tsc": {
			"outputs": []
		},
		"lint": {
			"outputs": []
		},
		"lint:fix": {
			"outputs": []
		},
		"dev:api": {
			"cache": false
		},
		"dev": {
			"cache": false
		},
		"start": {
			"cache": false
		}
	}
}
